{
  "name": "a-receipt",
  "version": "1.0.0",
  "description": "Receipt Generator Backend API with enhanced security and performance",
  "type": "module",
  "main": "index.js",
  "scripts": {
  "test": "mocha --recursive --exit",
    "start": "nodemon",
  "dev": "nodemon --env-file=.env.development",
  "worker": "node src/workers/runWorker.js",
    "prod": "node index.js",
  "prod:pm2": "pm2 start ecosystem.config.js --env production",
  "pm2:stop": "pm2 stop ecosystem.config.js || true",
  "crash-test": "node ./scripts/crash-test.js",
    "seed": "node ./scripts/seed.js",
    "find:orphans": "node ./scripts/find-orphan-documents.mjs",
  "backfill:notifications": "node ./scripts/backfill-notifications.js",
  "backfill:suites-name": "node ./scripts/backfill-suites-name.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs": "swagger-jsdoc -d swaggerDef.js -o swagger.json",
    "start:scheduler": "node ./scheduler/server.js",
    "security-check": "npm audit"
  },
  "keywords": [
    "receipt",
    "generator",
    "api",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.788.0",
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/client-textract": "^3.787.0",
    "@aws-sdk/credential-provider-env": "^3.775.0",
    "@fast-csv/format": "^5.0.2",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.6.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.4",
    "nvm": "^0.0.4",
    "puppeteer": "^24.8.0",
    "redis": "^4.7.0",
  "bullmq": "^1.84.0",
  "nodemailer": "^6.9.4",
  "handlebars": "^4.7.7",
  "sanitize-html": "^2.11.0",
  "juice": "^9.0.0",
  "node-cron": "^3.0.2",
  "redlock": "^4.2.0",
    "resend": "^4.0.1",
    "socket.io": "^4.8.1",
    "stripe": "^14.14.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
  "supertest": "^6.2.4",
  "mocha": "^10.2.0",
  "mongodb-memory-server": "^8.11.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
